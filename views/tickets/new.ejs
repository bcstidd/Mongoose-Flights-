
<%- include('./partials/header') %>

<form action="/tickets/<%= flight %>" method="POST">

    <h5><%= flight._id %></h5>
    
    
      <label>Seat:
        <input name="seat" required pattern="[A-F][1-9]\d?">
      </label><br>
    
      <label>Price:
        <input type="number" name="price">
      </label><br>
    
      <button type="submit">Add Ticket</button>
    
    </form>
    <%- include('./partials/footer') %>